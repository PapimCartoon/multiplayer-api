<introConfig>
	<configURL>XML/config2.XML</configURL>
	<graphics>
		<gameDisplayName>cool game</gameDisplayName><!--The name of the game that will be displayed-->
		<bigGameThumb></bigGameThumb><!--The URL the game will load as the background for the intro screen-->
		<smallGameThumb></smallGameThumb><!--The URL the game will load as a side image-->
		<introLogoURL></introLogoURL><!--The URL of the logo that is connected to the owner-->
	</graphics>
	<user_info>
	  <name>ofir the cool</name>
	  <avatar_url>ofir.jpg</avatar_url>
	  <is_guest>false</is_guest>
	  <user_statistics_url>http://www.google.com</user_statistics_url>
	</user_info>
	<grabIt>url</grabIt>
	<moreGrabIt><!--All the grab it links-->	

	</moreGrabIt>
	<moreGames>	
		<game>
			<callerId>12</callerId><!--The callerId of the game -->
			<image>http://www.notus-online.com/catalog/pics/Sexy_Satin_With_Lacework_6_Pics_Underwear_Set.jpg</image><!--The URL of the image to load for the more game-->
			<url>http://www.notus-online.com/catalog/pics/Sexy_Satin_With_Lacework_6_Pics_Underwear_Set.jpg</url><!--The URL of the game-->	
		</game>
	</moreGames>

	<!--if this value is empty you can't invite a friend-->
	<inviteBuddyURL>http://www.google.com/</inviteBuddyURL>
	<moreGamesURL>http://www.google.com/</moreGamesURL>
	<!--if this value is not empty you must popout your game to play-->
	<popoutURL>asd</popoutURL>
	<!--if this value is not empty the intro screen will do the action specefied-->
	<autoMatchType></autoMatchType>


	<inviteAutoMatch>
		<tableID></tableID> <!-- The table id of the game you connect to-->
		<tablePassword></tablePassword><!-- The play password of the game you connect to-->
		<viewPassword></viewPassword><!-- The view password of the game you connect to-->
		<autoMatchString></autoMatchString><!-- The autoMatch string of the game you connect to-->
	</inviteAutoMatch>
	<userManagement>
		<registerURL>http://127.0.0.1/index.asp?action=register&</registerURL>
		<!-- put name=..&email=..&pass..&is_male..&date_of_birth..&first_name..&last_name 
		result=true/false
		-->
		
		<loginURL>http://127.0.0.1/index.asp?action=login&</loginURL>
		<!-- put email,pass & all distribution params.. 
		gets intro_config_url=..
		-->
		<logoutURL>http://127.0.0.1/index.asp?action=logout&</logoutURL>
		<!--  all distribution params.. 
		gets intro_config_url=..
		-->
		<validateEmailURL>http://127.0.0.1/index.asp?action=validate_email&</validateEmailURL>
		<!-- put email
		result=true/false
		-->
		<validateNameURL>http://127.0.0.1/index.asp?action=validate_name&</validateNameURL>
		<!-- put name=..
		result=true/false
		-->
	</userManagement>
</introConfig>